<?xml version='1.0' encoding='us-ascii'?>
	<!--
		DTD for a simple listTester
	-->
	<!ELEMENT TestsList (Test)*>
	<!ELEMENT Test (TestClassName, WhatTest, HowWorkTest, WhenTestPass)>
	<!ATTLIST Test 
	    name     CDATA    #REQUIRED
	    skip     CDATA    #REQUIRED
	>
	<!ELEMENT TestClassName (#PCDATA)>
	<!ELEMENT WhatTest (%inline;)*>
	<!ELEMENT HowWorkTest (%inline;)*>
	<!ELEMENT WhenTestPass (%inline;)*>
	<!ELEMENT em (%inline;)*>
	<!ELEMENT br (%inline;)*>
	<!ELEMENT b (%inline;)*>
	<!ELEMENT LI (%inline;)*>
	<!ELEMENT UL (%inline;)*>

